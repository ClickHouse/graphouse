#!/usr/bin/make -f

package=graphouse

%:
	dh $@

override_dh_clean:
	./gradlew clean
	dh_clean

override_dh_build:
	./gradlew build
	dh_build

override_dh_install:
	./gradlew installDist
	dh_install

override_dh_installinit:
	dh_installinit --name=graphouse --noscripts

override_dh_builddeb:
	dh_builddeb -- -Zgzip
